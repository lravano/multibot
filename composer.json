{
    "name": "made/multibot",
    "description": "This package is a wrapper for Multibot REST API",
    "authors": [
        {
            "name": "Lorenzo Ravano",
            "email": "lravano@mademarketing.it"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "~6.0"        
    }, 
    "autoload": {
        "psr-4": {
            "Made\\Multibot\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Made\\Multibot\\MultibotServiceProvider"
            ],
            "aliases": {
                "Datagun": "Made\\Multibot\\Facade"
            }
        }
    }   

}
